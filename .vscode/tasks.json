{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cargo",
            "command": "build",
            "args": [
                "--target",
                "x86_64-pc-windows-gnu",
            ],
            "problemMatcher": [
                "$rustc"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "label": "rust: cargo build win64"
        },
        {
            "label": "Build debug",
            "type": "shell",
            "command": "/bin/bash ./scripts/build.sh",
            "problemMatcher": [
                "$rustc"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "type": "shell",
            "command": "gitsync",
            "args": [
                "sync",
                "-u",
                "Администратор",
                "~/Documents/_onec_dbs/regex-addin-tests/storage",
                "${workspaceRoot}/example/src"
            ],
            "group": "none",
            "label": "oscript: gitsync sync"
        }
    ]
}
